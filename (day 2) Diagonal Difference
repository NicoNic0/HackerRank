a = 0
b = 0
x = 0
loop = (len(arr))
while loop > 0:
        x = (arr[a][b]) + x
        a = a + 1
        b = b + 1
        loop = loop - 1

c = 0 
d = (len(arr)-1)
y = 0
loop2 = len(arr)
while loop2 > 0:
        y = (arr[c][d]) + y
        c = c + 1
        d = d - 1
        loop2 = loop2 - 1
        
z = [x ,y]
z.sort(reverse=True)
result = (z[0]-z[1])
